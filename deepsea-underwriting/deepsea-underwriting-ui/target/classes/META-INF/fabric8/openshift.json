{
  "apiVersion" : "v1",
  "kind" : "List",
  "items" : [ {
    "apiVersion" : "v1",
    "kind" : "Service",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/vertx.svg",
        "prometheus.io/port" : "9779",
        "prometheus.io/scrape" : "true"
      },
      "labels" : {
        "expose" : "true",
        "app" : "deepsea-underwriting-ui",
        "provider" : "fabric8",
        "version" : "1.0-SNAPSHOT",
        "group" : "com.assurant"
      },
      "name" : "deepsea-underwriting-ui"
    },
    "spec" : {
      "ports" : [ {
        "name" : "http",
        "port" : 8080,
        "protocol" : "TCP",
        "targetPort" : 8080
      } ],
      "selector" : {
        "app" : "deepsea-underwriting-ui",
        "provider" : "fabric8",
        "group" : "com.assurant"
      }
    }
  }, {
    "apiVersion" : "apps.openshift.io/v1",
    "kind" : "DeploymentConfig",
    "metadata" : {
      "annotations" : {
        "fabric8.io/iconUrl" : "img/icons/vertx.svg",
        "fabric8.io/metrics-path" : "dashboard/file/kubernetes-pods.json/?var-project=deepsea-underwriting-ui&var-version=1.0-SNAPSHOT"
      },
      "labels" : {
        "app" : "deepsea-underwriting-ui",
        "provider" : "fabric8",
        "version" : "1.0-SNAPSHOT",
        "group" : "com.assurant"
      },
      "name" : "deepsea-underwriting-ui"
    },
    "spec" : {
      "replicas" : 1,
      "revisionHistoryLimit" : 2,
      "selector" : {
        "app" : "deepsea-underwriting-ui",
        "provider" : "fabric8",
        "group" : "com.assurant"
      },
      "strategy" : {
        "rollingParams" : {
          "timeoutSeconds" : 3600
        },
        "type" : "Rolling"
      },
      "template" : {
        "metadata" : {
          "annotations" : {
            "fabric8.io/iconUrl" : "img/icons/vertx.svg",
            "fabric8.io/metrics-path" : "dashboard/file/kubernetes-pods.json/?var-project=deepsea-underwriting-ui&var-version=1.0-SNAPSHOT"
          },
          "labels" : {
            "app" : "deepsea-underwriting-ui",
            "provider" : "fabric8",
            "version" : "1.0-SNAPSHOT",
            "group" : "com.assurant"
          }
        },
        "spec" : {
          "containers" : [ {
            "env" : [ {
              "name" : "JAVA_OPTIONS",
              "value" : "-Dvertx.cacheDirBase=/tmp -Dvertx.jgroups.config=default-configs/default-jgroups-kubernetes.xml -Djava.net.preferIPv4Stack=true"
            }, {
              "name" : "JAVA_ARGS",
              "value" : "-Dvertx.cacheDirBase=/tmp -Dvertx.jgroups.config=default-configs/default-jgroups-kubernetes.xml -Djava.net.preferIPv4Stack=true -cluster"
            }, {
              "name" : "KUBERNETES_NAMESPACE",
              "valueFrom" : {
                "fieldRef" : {
                  "fieldPath" : "metadata.namespace"
                }
              }
            } ],
            "image" : "deepsea-underwriting-ui:latest",
            "imagePullPolicy" : "IfNotPresent",
            "name" : "vertx",
            "ports" : [ {
              "containerPort" : 8080,
              "name" : "http",
              "protocol" : "TCP"
            }, {
              "containerPort" : 9779,
              "name" : "prometheus",
              "protocol" : "TCP"
            }, {
              "containerPort" : 8778,
              "name" : "jolokia",
              "protocol" : "TCP"
            } ],
            "securityContext" : {
              "privileged" : false
            }
          } ]
        }
      },
      "triggers" : [ {
        "type" : "ConfigChange"
      }, {
        "imageChangeParams" : {
          "automatic" : true,
          "containerNames" : [ "vertx" ],
          "from" : {
            "kind" : "ImageStreamTag",
            "name" : "deepsea-underwriting-ui:latest"
          }
        },
        "type" : "ImageChange"
      } ]
    }
  }, {
    "apiVersion" : "v1",
    "kind" : "Route",
    "metadata" : {
      "labels" : {
        "app" : "deepsea-underwriting-ui",
        "provider" : "fabric8",
        "version" : "1.0-SNAPSHOT",
        "group" : "com.assurant"
      },
      "name" : "deepsea-underwriting-ui"
    },
    "spec" : {
      "port" : {
        "targetPort" : "http"
      },
      "to" : {
        "kind" : "Service",
        "name" : "deepsea-underwriting-ui",
        "weight" : 100
      }
    }
  } ]
}